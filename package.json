    "chai-subset": "1.6.0",
    "codecov": "3.5.0",
    "conventional-changelog-metahub": "2.0.2",
    "cz-conventional-changelog": "2.1.0",
    "mocha": "6.1.4",
    "nyc": "14.1.1",
    "semantic-release": "15.13.17",
    "standard": "14.0.2",
